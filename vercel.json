{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.5.1"
    }
  },
  "redirects": [
	{ "source": "/api/(.*)",  "destination": "/" },
	{ "source": "/(.*).md",  "destination": "/" },
	{ "source": "/.(.*)",  "destination": "/" }
  ],
  "rewrites": [
    { "source": "/",  "destination": "/api/index.php" }
  ]
}
