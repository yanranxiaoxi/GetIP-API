{
	"functions": {
		"index.php": {
			"runtime": "vercel-php@0.5.2"
		}
	},
	"redirects": [
		{ "source": "/api/(.*).php",  "destination": "/" },
		{ "source": "/(.*).md",  "destination": "/" },
		{ "source": "/.(.*)",  "destination": "/" }
	],
	"rewrites": [
		{ "source": "/",  "destination": "/api/index.php" }
	]
}
